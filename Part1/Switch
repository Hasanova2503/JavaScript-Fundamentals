let color  = 'yellow';

switch(color){

  case 'red':
   console.log('Color is red');
   break;
   case 'blue':
   console.log('color is blue');
   break;
   default:
   console.log('Color is not red or blue');
   break;
}


let day ;


switch(new Date().getDay()){

case 0:
 day = 'Sunday';
 break;
 case 1:
 day ='Monday'
 break;

}